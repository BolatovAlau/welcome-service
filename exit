[32m+[m[32musing System.Diagnostics;[m
[32m+[m[32musing Microsoft.AspNetCore.Mvc;[m
[32m+[m[32musing mywebapp.Models;[m
[32m+[m
[32m+[m[32mnamespace mywebapp.Controllers;[m
[32m+[m
[32m+[m[32mpublic class HomeController : Controller[m
[32m+[m[32m{[m
[32m+[m[32m    private readonly ILogger<HomeController> _logger;[m
[32m+[m
[32m+[m[32m    public HomeController(ILogger<HomeController> logger)[m
[32m+[m[32m    {[m
[32m+[m[32m        _logger = logger;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public IActionResult Index()[m
[32m+[m[32m    {[m
[32m+[m[32m        return View();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public IActionResult Privacy()[m
[32m+[m[32m    {[m
[32m+[m[32m        return View();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    [ResponseCache(Duration = 0, Location = ResponseCacheLocation.None, NoStore = true)][m
[32m+[m[32m    public IActionResult Error()[m
[32m+[m[32m    {[m
[32m+[m[32m        return View(new ErrorViewModel { RequestId = Activity.Current?.Id ?? HttpContext.TraceIdentifier });[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
